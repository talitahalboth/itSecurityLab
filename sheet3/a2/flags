     0, 000000000000
     1, 000000000001
     2, 000000000010
  1024, 010000000000
   512, 001000000000
    64, 000001000000
   128, 000010000000

#define O_RDONLY 0
#define O_WRONLY 1
#define O_RDWR 2
#define O_APPEND 1024
#define O_TRUNC 512
#define O_CREAT 64
#define O_EXCL 128

if(!(oflags & O_RDONLY || oflags & O_RDWR))
oflags = 0

